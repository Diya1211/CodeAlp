import pandas as pd
import numpy as np
import matplotlib.pyplot as plt
import seaborn as sns
df = pd.read_csv("unemployment_data.csv")

df.head()

df.isnull().sum()

df = df.dropna()

df['Date'] = pd.to_datetime(df['Date'])
df['Year'] = df['Date'].dt.year
df['Month'] = df['Date'].dt.month

df['Unemployment_Rate'].describe()

yearly_avg = df.groupby('Year')['Unemployment_Rate'].mean()
yearly_avg

plt.figure(figsize=(10,5))
plt.plot(df['Date'], df['Unemployment_Rate'])
plt.xlabel("Year")
plt.ylabel("Unemployment Rate (%)")
plt.title("Unemployment Trend Over Time")
plt.show()

plt.figure(figsize=(10,5))
sns.boxplot(x=df['Year'] >= 2020, y=df['Unemployment_Rate'])
plt.xlabel("Pre-COVID vs Post-COVID")
plt.ylabel("Unemployment Rate (%)")
plt.title("Impact of COVID-19 on Unemployment")
plt.show()

monthly_avg = df.groupby('Month')['Unemployment_Rate'].mean()

plt.figure(figsize=(8,4))
monthly_avg.plot(kind='bar')
plt.xlabel("Month")
plt.ylabel("Average Unemployment Rate")
plt.title("Seasonal Unemployment Pattern")
plt.show()


